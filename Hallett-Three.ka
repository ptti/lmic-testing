//
// Assumption Set Three: Viral load is related to infectiousness and symptoms
//
%var:	beta		0.034           // probability of infection from contact
%var:	beta_low	0.5*beta	// infectiousness for low viral load people
%var:	beta_high	beta		// infectiousness for high viral load people
%var:	p_high		1.0		// chance of low viral load people developing symptoms
%var:	p_low		0.0		// chance of high viral load people developing symptoms
